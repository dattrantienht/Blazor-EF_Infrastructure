<CascadingAuthenticationState>
    <Router AppAssembly="@typeof(Program).Assembly"> @*PreferExactMatches="@true" cause error when publish the app*@
        <Found Context="routeData">
            <AuthorizeRouteView RouteData="@routeData" DefaultLayout="@typeof(AdminLayout)" />
        </Found>
        <NotFound>
            <LayoutView Layout="@typeof(AdminLayout)">
                <p>Sorry, there's nothing at this address.</p>
            </LayoutView>
        </NotFound>
    </Router>
</CascadingAuthenticationState>
